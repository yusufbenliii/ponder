#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules/@wagmi/cli/dist/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules/@wagmi/cli/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules/@wagmi/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules/@wagmi/cli/dist/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules/@wagmi/cli/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules/@wagmi/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/@wagmi+cli@1.5.2_typescript@5.0.4/node_modules:/Users/yusufbenli/projects/parabol/ponder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@wagmi/cli/dist/cli.js" "$@"
else
  exec node  "$basedir/../@wagmi/cli/dist/cli.js" "$@"
fi
